      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Resetting position
if instance_exists(obj_play){
    if obj_play.inEditor = 1{
        if select = 0{
            x = xSet;
            y = ySet;
        }
        //Reset values
        hspeed = 0;
        direct = 0;
        gravity = 0;
        dead = 0;
        visible = true;
        sprite_index = liveSpr;
        turn = 0;
        kick = 0;
        kickDeadly = 0;
        image_speed = 0;
        bossMove = 1;
        bossFire = 1;
        bossDir = 0;
        shootLoop = 0;
        bossJump = 0;
        
        if fromBlock = 1{
            instance_destroy();
        }
        
    } else {
        //Outside of bounds
        if x &gt; view_xview[2]+488 || x &lt; view_xview[2]-8{
            hspeed = 0;
            gravity = 0;
            vspeed = 0;
            
            if bill = 1{
                instance_destroy();
            }
        } else {
            if boss = 0{
                if boo = 0 &amp;&amp; bill = 0{
                    //Gravity
                    if !collision_point(x,y+9,obj_solidtop,true,false){
                        gravity = 0.16;
                    }
                    if !collision_point(x,y+9,obj_block_parent,true,false){
                        gravity = 0.16;
                    }
                    if !collision_point(x,y+9,obj_spring,true,false){
                        gravity = 0.16;
                    } else {
                        if vspeed &gt; 0{
                            vspeed = 0;
                        }
                    }
                    //Terminal velocity
                    if vspeed &gt; 2{
                        vspeed = 2;
                    }
                    
                    if dead = 0{
                        if billFire = 0{
                            //Moving left
                            if direct = 0{
                                hspeed = -0.35;
                                image_xscale = 1;
                            }
                            //Moving right
                            if direct = 1{
                                hspeed = 0.35;
                                image_xscale = -1;
                            }
                        }
                    }
                }
            } else {
                if boss = 1{
                    //Bowser code
//                    if bossMove != 0{
                        //Stepping around
                        if obj_player.x &gt; self.x{
                            if bossDir = 0{
                                hspeed = 0.45;
                            } else {
                                hspeed = -0.45;
                            }
                            image_xscale = -1;
                        }
                        if obj_player.x &lt; self.x{
                            if bossDir = 0{
                                hspeed = -0.45;
                            } else {
                                hspeed = 0.45;
                            }
                            image_xscale = 1;
                        }
                        
                        if bossMove = 1{
                            bossMove = 2;
                            alarm[4] = 90;
                        }
                        
                        //Shooting
                        if bossFire != 0{
                            sprite_index = fireSpr;
                            if bossFire = 1{
                                alarm[5] = 120;
                                bossFire = 2;
                            }
                        } else {
                            sprite_index = liveSpr;
                        }
                        
                        //Handling step loop
                        if shootLoop = 0{
                            shootLoop = 1;
                            alarm[6] = 40;
                        }
                        
                        //Jumping
                        if bossJump = 0{
                            bossJump = 1;
                            alarm[7] = 220;
                        }
                        
                        //Gravity
                        if !collision_point(x,y+17,obj_solidtop,true,false){
                            gravity = 0.2;
                        }
                        if !collision_point(x,y+17,obj_block_parent,true,false){
                            gravity = 0.2;
                        }
                        if !collision_point(x,y+17,obj_spring,true,false){
                            gravity = 0.2;
                        }
                        //Terminal velocity
                        if vspeed &gt; 2{
                            vspeed = 2;
                        }
                        
                        //Side collisions
                        if collision_rectangle(x-16,y-24,x,y,obj_solid,true,false){ //Towards the player
                            if bossDir = 0{
                                hspeed = 0;
                            }
                        }
                        if bossDir = 1{
                            if collision_rectangle(x+16,y-24,x,y,obj_solid,true,false){ //Away from the player
                                hspeed = 0;
                                x -= 1;
                            }
                        }
                        if collision_rectangle(x+16,y-24,x,y,obj_solid,true,false){
                            if bossDir = 0{
                                hspeed = 0;
                            }
                        }
                        if bossDir = 1{
                            if collision_rectangle(x-16,y-24,x,y,obj_solid,true,false){
                                hspeed = 0;
                                x += 1;
                            }
                        }
                        
                        //Top collisions
                        if collision_rectangle(x-16,y-24,x+16,y,obj_solid,false,false){
                            vspeed = 1;
                        }
                }
            }
            
            //Shells turning at blocks
            if dead = 4{
                //Moving left
                if kick = 1{
                    if direct = 0{
                        hspeed = -3.3;
                        image_speed = -0.3;
                    }
                    //Moving right
                    if direct = 1{
                        hspeed = 3.3;
                        image_speed = 0.3;
                    }
                }
            }
            
            //Image speed
            if dead = 0{
                image_speed = 0.1;
            }
    
            //Collisions
            if boss = 0{
                if collision_point(x-8,y,obj_solid,false,false){
                    //Change direction
                    direct = 1;
                }
                if collision_point(x-8,y,obj_block_parent,false,false){
                    //Change direction
                    direct = 1;
                }
                if collision_point(x+8,y,obj_solid,false,false){
                    //Change direction
                    direct = 0;
                }
                if collision_point(x+8,y,obj_block_parent,false,false){
                    //Change direction
                    direct = 0;
                }
            }
        }
    }
}

//Dragging
/*if select = 1{    
    x = ((mouse_x div 16)*16)+8;
    y = ((mouse_y div 16)*16)+8;
}*/

if select = 1{
    if released = 0{
        x = ((mouse_x div 16)*16)+8;
        y = ((mouse_y div 16)*16)+8;
    } else {
        x = mouse_x+8;
        y = mouse_y-8;
        global.selected = 1;
    }
    depth = -1000;
} else {
    selOff = 0;
    depth = baseDepth;
}

//Dying
if dead = 1{
    //Stomping
    hspeed = 0;
    
    if bill = 0{
        alarm[0] = 30;
    }
    
    if bill = 1{
        vspeed = 2;
    }
    
    dead = 3;
    //Death animation
    sprite_index = deadSpr;
}

if dead = 2{
    //Item death
    hspeed = 0;
    vspeed = -2;
    dead = 3;
    fallY = 1;
}
if fallY = 1{
    if vspeed &gt; 0{
        image_yscale = -1;
    }
    gravity = 0.23;
}

if dead = 4{
    //Shell
    sprite_index = deadSpr;
    if kick = 0{
        hspeed = 0;
        image_speed = 0;
    }
    
    if hspeed = 0{
        //kickDeadly = 0;
    }
}

//Being stacked
if bill = 0{
    if (boo = 1 &amp;&amp; obj_play.inEditor = 1) || (boo = 0){
        if collision_point(x,y+9,obj_enemy,true,true){
            gravity = 0;
            x = collision_point(x,y+9,obj_enemy,true,false).x;
            hspeed = 0;
            stacked = 1;
        }
    }
}

//Turning at the end of the floor
if dead = 0{
    if walkOff = 1{
        if hspeed &gt; 0{
            if !place_meeting(x+16,y+16,obj_solidtop) &amp;&amp; !place_meeting(x+16,y+16,obj_block_parent){
                direct = 0;
            }
        } else {
            if !place_meeting(x-16,y+16,obj_solidtop) &amp;&amp; !place_meeting(x-16,y+16,obj_block_parent){
                direct = 1;
            }
        }
    }
}

</string>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Resetting position
if instance_exists(obj_play){
    if obj_play.inEditor = 1{
        if select = 0{
            x = xSet;
            y = ySet;
        }
        //Reset values
        hspeed = 0;
        direct = 0;
        gravity = 0;
        dead = 0;
        visible = true;
        turn = 0;
        image_speed = 0;
        collected = 0;
    } else {
        //Gravity
        if !collision_point(x,y+9,obj_solidtop,true,false) &amp;&amp; !collision_point(x,y+9,obj_block_parent,true,false){
            gravity = 0.16;
        } else {
            if collision_point(x,y+9,obj_block_parent,true,false).vspeed &gt;= 0{
                vspeed = 0;
                gravity = 0;
                if billFire = 2{
                    billFire = 0;
                    if mushroom = 0{
                        hspeed = 0;
                    } else {
                        if hspeed &lt; 0{
                            hspeed = -0.35;
                        } else {
                            hspeed = 0.35;
                        }
                    }
                }
            } else {
                vspeed = -2.4;
            }
        }
            
        //Terminal velocity
        if vspeed &gt; 2{
            vspeed = 2;
        }
        
        if billFire = 0{
            if mushroom = 1{
                //Moving left
                if direct = 0{
                    hspeed = -0.35;
                }
                //Moving right
                if direct = 1{
                    hspeed = 0.35;
                }
            }
        }

        //Collisions
        if collision_point(x-8,y,obj_solid,false,false){
            //Change direction
            direct = 1;
        }
        if collision_point(x+8,y,obj_solid,false,false){
            //Change direction
            direct = 0;
        }
    }
}

//Dragging
if select = 1{
    if released = 0{
        x = ((mouse_x div 16)*16)+8;
        y = ((mouse_y div 16)*16)+8;
    } else {
        x = mouse_x+8;
        y = mouse_y-8;
    }
    depth = -1000;
} else {
    selOff = 0;
    depth = baseDepth;
}

//Collected
if collected = 1{
    visible = false;
} else {
    visible = true;
}

</string>
          </argument>
        </arguments>
      </action>

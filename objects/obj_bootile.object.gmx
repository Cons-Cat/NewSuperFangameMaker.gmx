            <string>//Checking to see if the Boo can chase the player
if instance_exists(obj_player){

if obj_play.inEditor = 0{
    if obj_player.x &lt;= x{
        if obj_player.image_xscale = -1{
            canAttack = 1;
            image_index = 5;
            image_xscale = 1;
        } else {
            canAttack = 1;
            image_xscale = -1;
            image_index = index;
        }
    }
} else {
    canAttack = 0;
}


if obj_player.x &lt;= x{
        if obj_player.image_xscale = 1{
            canAttack = 0;
            image_index = 5;
            image_xscale = 1;
        }
}

if obj_player.x &gt; x{
    if obj_player.image_xscale = -1{
        canAttack = 0;
        image_index = 5;
        image_xscale = -1;
    } else {
        canAttack = 1;
        image_xscale = -1;
        image_index = index;
    }
}

//Moving toward the player/stopping
if !(x &gt; view_xview[2]+480 || x &lt; view_xview[2]){
    if canAttack = 1{
        image_index = 0;
        move_towards_point(obj_player.x,obj_player.y,spd);
        if spd &lt; 0.5{
            spd += 0.025;
        }
    }
    
    if canAttack = 0{
        if spd &gt; 0{
            spd -= 0.025;
        }
    }

} else {
    spd = 0;
}

speed = spd;
} else {
    speed = 0;
}


if shake = 0{
    if obj_play.inEditor = 1{
        if select = 1{
            image_index = 5;
        } else {
            image_index = index;
        }
    }
}

//Shaking
if asdf = 0{
    if shake = 1{
        image_index = index;
        asdf = 1;
        shake = 0;
        
        if index != 4{
            index += 1;
        } else {
            index = 0;
        }
    }
}

if select = 0{
    asdf = 0;
}


//Selecting the skin
if global.skin = 0{
    sprite_index = spr_smb1_bootile;
    image_speed = 0;
    flip = 0;
    imgs = 0.04;
}
if global.skin = 1{

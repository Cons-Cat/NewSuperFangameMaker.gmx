        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Mario's offset when riding
if ride = 1{
    x = obj_player.x;
    y = obj_player.y;
    image_xscale = obj_player.image_xscale;
    
    if image_xscale = 1{
        obj_player.rideX = self.rideX-6;
    } else {
        obj_player.rideX = self.rideX-4;
    }
    obj_player.rideY = self.rideY+jumpY+bumpOff;
    
    
    if obj_player.hspeed != 0{
        if obj_player.jumpnow = 0{
            sprite_index = sprMove;
        }
        image_speed = obj_player.hspeed/10;
    } else {
        if obj_player.jumpnow = 0{
            sprite_index = sprStill;
        }
    }
    if obj_player.jumpnow = 1{
        sprite_index = sprJump;
        jumpY = jumpYOff;
        image_index = 0;
    } else {
        jumpY = 0;
    }
} else {
    if obj_player.jumpnow = 0{
        sprite_index = sprStill;
    }
}

//Resetting position
if obj_play.inEditor = 1{
    if select = 0{
        x = xSet;
        y = ySet;
        ride = 0;
        obj_player.rideX = 0;
        obj_player.rideY = 0;
        obj_player.JumpY = 0;
        obj_player.bumpOff = 0;
        sprite_index = sprStill;
    }
    image_speed = 0;
    image_index = 0;
}

//Gravity and terminal velocity(max fall speed)
if obj_play.inEditor = 0{
    if ride = 0{
        if !collision_rectangle(x-7,y,x+7,y+17,obj_solidtop,false,false){
            if !collision_rectangle(x-6,y+9,x+6,y+17,obj_block_parent,false,true){
                gravity = 0.16;
                sprite_index = sprJump;
            }
        }
    }
}

//Max downward speed
if ride = 0{
    if vspeed &gt; 2.5 {
        vspeed = 2.5;
    }
}

//Dragging
if select = 1{    
    x = ((mouse_x div 16)*16)+8;
    y = ((mouse_y div 16)*16);
}

</string>
